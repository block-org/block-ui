{
  "name": "@block-org/block-ui",
  "version": "0.0.0",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "clean": "rimraf lib esm dist",
    "build:css": "block-scripts build:component:css",
    "build:esm": "block-scripts build:component:esm",
    "build:cjs": "block-scripts build:component:cjs",
    "build": "npm run clean && npm run build:esm && npm run build:css",
    "eslint": "eslint components/ --fix --cache --quiet --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint 'components/**/*.less'",
    "formatter ": "prettier --config .prettierrc --write \"{stories,tests,components}/**/*.{js,jsx,ts,tsx}\"",
    "color": "node ./scripts/compileColors.js",
    "icon": "cd icon && rimraf react-icon/ && rimraf react-icon-cjs && cd build/ && node generate.js",
    "release": "npx ts-node ./scripts/release.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "typings": "lib/index.d.ts",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "sideEffects": [
    "dist/*",
    "esm/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "files": [
    "dist",
    "esm",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/runtime": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@block-org/block-scripts": "^0.0.2",
    "@block-org/color": "^0.0.1",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/inquirer": "^8.1.3",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "^8.2.3",
    "child_process": "^1.0.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fs-extra": "^7.0.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "lint-staged": "^12.0.2",
    "nunjucks": "^3.2.2",
    "postcss-less": "4.0.1",
    "prettier": "^2.5.0",
    "pretty-quick": "^3.1.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "semver": "^7.3.5",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "svgo": "^2.3.0",
    "typescript": "^4.4.4",
    "util": "^0.12.4"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.4.2"
  }
}